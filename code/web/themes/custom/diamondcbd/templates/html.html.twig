{% extends '@cpl_base/html.html.twig' %}

{% block favicon %}
  <link rel="apple-touch-icon" sizes="180x180" href="/{{ directory }}/images/favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/{{ directory }}/images/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/{{ directory }}/images/favicon/favicon-16x16.png">
  <link rel="manifest" href="/{{ directory }}/images/favicon/site.webmanifest">
  <link rel="mask-icon" href="/{{ directory }}/images/favicon/safari-pinned-tab.svg" color="#5bbad5">
  <link rel="shortcut icon" href="/{{ directory }}/images/favicon/favicon.ico">
  <meta name="msapplication-TileColor" content="#ffffff">
  <meta name="msapplication-config" content="/{{ directory }}/images/favicon/browserconfig.xml">
  <meta name="theme-color" content="#ffffff">
{% endblock %}
